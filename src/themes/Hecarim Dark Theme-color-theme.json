				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#FF9E64"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
