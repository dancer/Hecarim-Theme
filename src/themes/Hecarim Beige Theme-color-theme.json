	"tokenColors": [
		{
			"settings": {
				"background": "#EDE9D0",
				"foreground": "#3E2723"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#795548"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#1B5E20"
			}
		},
		{
			"name": "Regular Expressions and Escape Characters",
			"scope": [
				"string.regexp",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#006064"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
